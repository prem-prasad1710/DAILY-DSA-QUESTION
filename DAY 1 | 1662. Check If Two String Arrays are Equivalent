/**
 * @param {string[]} word1
 * @param {string[]} word2
 * @return {boolean}
 */
var arrayStringsAreEqual = function(word1, word2) {
    let word="";
    let match="";
    for (let x =0;x<word1.length;x++){
        word = word+word1[x];
    }
    for (let y = 0;y<word2.length;y++){
        match  = match+ word2[y];
    }
    if(word === match){
        return true;

    }
    else {
        return false;

    }
};
